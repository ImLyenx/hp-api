generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Card {
  id    Int     @id @default(autoincrement())
  name  String
  slug  String  @unique
  tags  String
  house String? @db.VarChar(32)
  image String
}
